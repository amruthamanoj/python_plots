import matplotlib.pyplot as plt
from mpl_toolkits.mplot3d import Axes3D
import numpy as np

x = np.arange(0.2, 3.2, 0.2)
y = np.arange(0.1, 1.0, 0.1)

X, Y = np.meshgrid(x, y)
Z = 1 - (X**2) - (Y**2)

fig = plt.figure()
ax = Axes3D(fig)
ax.plot_surface(X, Y, Z)
plt.xlabel('b')
plt.ylabel('d')
plt.show()
